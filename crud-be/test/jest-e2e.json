{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "crud-be/directory/(.*)": "<rootDir>/../libs/directory/src/$1",
    "crud-be/directory": "<rootDir>/../libs/directory/src",
    "crudBE/test/(.*)": "<rootDir>/../libs/test/src/$1",
    "crudBE/test": "<rootDir>/../libs/test/src"
  }
}