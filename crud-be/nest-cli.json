{
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "outDir": "./dist",
  "compilerOptions": {
    "plugins": [
      "@nestjs/swagger/plugin"
    ],
    "webpack": true,
    "tsConfigPath": "tsconfig.build.json",
    "outDir": "./dist"
  },
  "projects": {
    "auth": {
      "type": "library",
      "root": "libs/auth",
      "entryFile": "index",
      "sourceRoot": "libs/auth/src",
      "compilerOptions": {
        "tsConfigPath": "libs/auth/tsconfig.lib.json"
      }
    },
    "users": {
      "type": "library",
      "root": "libs/users",
      "entryFile": "index",
      "sourceRoot": "libs/users/src",
      "compilerOptions": {
        "tsConfigPath": "libs/users/tsconfig.lib.json"
      }
    },
    "projects": {
      "type": "library",
      "root": "libs/projects",
      "entryFile": "index",
      "sourceRoot": "libs/projects/src",
      "compilerOptions": {
        "tsConfigPath": "libs/projects/tsconfig.lib.json"
      }
    },
    "impacts": {
      "type": "library",
      "root": "libs/impacts",
      "entryFile": "index",
      "sourceRoot": "libs/impacts/src",
      "compileOptions": {
        "tsConfigPath": "libs/impacts/tsconfig.lib.json"
      }
    },
    "attackType": {
      "type": "library",
      "root": "libs/attack-type",
      "entryFile": "index",
      "sourceRoot": "libs/attack-type/src",
      "compileOptions": {
        "tsConfigPath": "libs/attack-type/tsconfig.lib.json"
      }
    },
    "hardware": {
      "type": "library",
      "root": "libs/hardware",
      "entryFile": "index",
      "sourceRoot": "libs/hardware/src",
      "compileOptions": {
        "tsConfigPath": "libs/hardware/tsconfig.lib.json"
      }
    },
    "connections": {
      "type": "library",
      "root": "libs/connections",
      "entryFile": "index",
      "sourceRoot": "libs/connections/src",
      "compileOptions": {
        "tsConfigPath": "libs/connections/tsconfig.lib.json"
      }
    }
  }
}